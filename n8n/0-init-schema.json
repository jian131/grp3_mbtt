      "parameters": {
        "operation": "executeQuery",
        "query": "CREATE TABLE IF NOT EXISTS listings (id SERIAL PRIMARY KEY, title TEXT, city TEXT, district TEXT, ward TEXT, price_million NUMERIC, area_m2 NUMERIC, rent_per_sqm_million NUMERIC, lat DOUBLE PRECISION, lon DOUBLE PRECISION, geom GEOMETRY(POINT, 4326), type TEXT, segment TEXT, frontage NUMERIC, floors INTEGER, views INTEGER DEFAULT 0, saved_count INTEGER DEFAULT 0, image_url TEXT, raw_data JSONB, created_at TIMESTAMP DEFAULT NOW());",
        "options": {}
      },
      "id": "table-1",
      "name": "Create Listings Table",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.5,
      "position": [
        680,
        320
      ],
      "credentials": {
        "postgres": {
          "id": "1",
          "name": "JFinder DB"
        }
      }
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "CREATE INDEX IF NOT EXISTS idx_listings_geom ON listings USING GIST (geom); CREATE INDEX IF NOT EXISTS idx_listings_price ON listings (price_million); CREATE INDEX IF NOT EXISTS idx_listings_city ON listings (city); CREATE INDEX IF NOT EXISTS idx_listings_district ON listings (district);",
        "options": {}
      },
      "id": "idx-1",
